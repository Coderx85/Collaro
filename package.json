  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "npx oxlint && next lint",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "docker:dev": "docker compose -f src/dev/compose.yaml",
    "with-dev-env": "dotenv -e .env.development -- ",
    "with-prod-env": "dotenv -e .env.production -- ",
    "analyze": "ANALYZE=true npm run build",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:reset": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "cd ../../packages/database && drizzle-kit studio",
    "docker:run": "node server.js"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
